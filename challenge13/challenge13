wrapGifts(["ğŸ“·", "âš½ï¸"])
/* Resultado:
[ '****',
  '*ğŸ“·*',
  '*âš½ï¸*',
  '****'
]
*/

wrapGifts(["ğŸˆğŸ¸", "ğŸ®ğŸ§¸"])
/* Resultado:
[ '******',
  '*ğŸˆğŸ¸*',
  '*ğŸ®ğŸ§¸*',
  '******'
]
*/

wrapGifts(["ğŸ“·"])
/* Resultado:
[ '****',
  '*ğŸ“·*',
  '****'
]
*/

wrapGifts([])

function wrapGifts(gifts) {
  /*ğŸ“·.length =>> 2*/
  let wrap = []
  let wrapping_paper = '*'
  if(gifts != ''){
    wrap.push(wrapping_paper.repeat(gifts[0].length+2))
  	gifts.map(e =>{
    	wrap.push(wrapping_paper+e+wrapping_paper)
  	})
  	wrap.push(wrapping_paper.repeat(gifts[0].length+2))
  }
  
   return wrap
}
  
 
